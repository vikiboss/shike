{
  "name": "shike",
  "type": "module",
  "version": "0.0.1",
  "description": "Zero dependency & pure date-time formatting library, nothing redundant.",
  "author": "Viki <hi@viki.moe> (https://github.com/vikiboss)",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "date",
    "time",
    "date-format",
    "time-format",
    "formatter",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prerelease": "unbuild",
    "release": "npm publish"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "unbuild": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
